generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id        String   @id @default(uuid())
  name      String
  phone     String   @unique
  email     String
  orders    orders[]
  createdAt DateTime @default(now())
}

model orders {
  id        String   @id @default(uuid())
  product   Int
  quantity  Int
  price     Float
  address   String
  comment   String
  status    String   @default("open")
  delivery  DateTime
  userId    String
  user      users    @relation(fields: [userId], references: [id])
  createdAt DateTime @default(now())
}

model subscriptions {
  id        String   @id @default(uuid())
  email     String
  createdAt DateTime @default(now())
}

model eggs_yield {
  id        Int      @id @default(autoincrement())
  chickens  Int
  quantity  Int
  comment   String?
  date      DateTime @unique

}


